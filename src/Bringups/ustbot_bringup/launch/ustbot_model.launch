<launch>
  <arg name="model" default="ustbot"/>
  <arg name="multi_robot_name" default=""/>

  <include file="$(find ustbot_bringup)/launch/ustbot_remote.launch">
    <arg name="model" value="$(arg model)" />
    <arg name="multi_robot_name" value="$(arg multi_robot_name)"/>
  </include>
  
  
  <!-- These nodes is just for debugging/making sure the joints move accordingly

  <node pkg="joint_state_publisher" type="joint_state_publisher" name="joint_state_publisher">
    <param name="use_gui" value="false"/>
    <param name="rate" value="50"/>
  </node>
  
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ustbot_description)/rviz/ustbot.rviz"/>
  -->
</launch>
